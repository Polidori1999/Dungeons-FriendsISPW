            <TableColumn fx:id="tableViewLobbyName" minWidth="0.0" prefWidth="191.3333282470703" text="Lobby name" />
            <TableColumn fx:id="tableViewMaxPlayers" prefWidth="84.66671752929688" text="Max players" />
            <TableColumn fx:id="tableViewLiveOrNot" prefWidth="144.66668701171875" text="Live/Online" />
            <TableColumn fx:id="tableViewDuration" prefWidth="207.33331298828125" text="Duration">
               <cellValueFactory>
                  <PropertyValueFactory property="editButton" />
               </cellValueFactory>
            </TableColumn>
            <TableColumn fx:id="tableViewLobbyEdit" prefWidth="113.3333740234375" text="Edit" />
            <TableColumn fx:id="tableViewLobbyDelete" prefWidth="114.66656494140625" text="Delete" />
